[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/DataQuestPro-BE
ExecStart=/home/ubuntu/venv/bin/gunicorn \
        --workers 3 \
        --bind unix:/tmp/gunicorn.sock \
        app.wsgi:application

[Install]
WantedBy=multi-user.target


